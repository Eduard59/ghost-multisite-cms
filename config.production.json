{
  "url": "https://ghost-cms-multisite.onrender.com",
  "server": {
    "port": 2368,
    "host": "0.0.0.0"
  },
  "database": {
    "client": "sqlite3",
    "connection": {
      "filename": "/var/lib/ghost/content/data/ghost.db"
    },
    "useNullAsDefault": true
  },
  "mail": {
    "transport": "Direct"
  },
  "logging": {
    "level": "info",
    "transports": ["stdout"]
  },
  "paths": {
    "contentPath": "/var/lib/ghost/content"
  },
  "privacy": {
    "useUpdateCheck": false,
    "useGravatar": false,
    "useRpcPing": false,
    "useStructuredData": true
  },
  "imageOptimization": {
    "resize": true,
    "srcsets": true
  },
  "compress": true,
  "preloadHeaders": true,
  "caching": {
    "frontend": {
      "maxAge": 0
    }
  },
  "portal": {
    "name": "DentalPrice AI",
    "button": true,
    "plans": ["free"],
    "signupButtonText": "Subscribe to Newsletter",
    "buttonIcon": "icon-1",
    "buttonStyle": "icon-and-text",
    "allowSelfSignup": true
  },
  "labs": {
    "collections": true,
    "members": true
  },
  "auth": {
    "type": "password"
  },
  "admin": {
    "redirects": false
  },
  "spam": {
    "user_login": {
      "minWait": 60,
      "maxWait": 300,
      "freeRetries": 10
    }
  }
}